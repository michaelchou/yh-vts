<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >

  <class name="com.yihuacomputer.cols.entity.Bank" table="Bank" dynamic-insert="true">
    <id name="id" type="int" column="id">
      <generator class="identity"/>
    </id>
    <property name="strBankCode" column="strBankCode" type="string" not-null="true" />
	<property name="strBankName" column="strBankName" type="string" not-null="true" />
	</class>
</hibernate-mapping>
