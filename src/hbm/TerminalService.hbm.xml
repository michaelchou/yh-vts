<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.yihuacomputer.cols.entity.TerminalService" table="TerminalService" lazy="false">
		<id name="id" type="int" column="id">
			<generator class="identity" />
		</id>
		<property name="terminalId" column="iTerminalId" type="int" />
		<property name="templateId" column="iTemplateId" type="int" />
	</class>
</hibernate-mapping>
