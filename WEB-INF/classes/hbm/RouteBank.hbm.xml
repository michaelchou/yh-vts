<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.yihuacomputer.cols.entity.RouteBank" table="RouteBank" lazy="false">
		<id name="id" type="int" column="id">
			<generator class="identity" />
		</id>
		<property name="strRouteCode" column="strRouteCode" type="string" />
		<property name="strBankCode" column="strBankCode" type="string" />
		<property name="strBankName" column="strBankName" type="string" />
		<property name="strBankType" column="strBankType" type="string" />
		<property name="strBankTypeCode" column="strBankTypeCode" type="string" />
		<property name="strSettleBankCode" column="strSettleBankCode" type="string" />
		<property name="strCCPCCode" column="strCCPCCode" type="string" />
		<property name="strSizeMark" column="strSizeMark" type="string" />
	</class>
</hibernate-mapping>
