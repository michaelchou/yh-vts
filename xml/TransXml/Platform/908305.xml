<?xml version="1.0" encoding="UTF-8"?>
<MappingRule description="短信通修改">
    <!-- id 唯一标识 -->
    <!-- strAlignType 定长域对齐方式，1：右对齐，2：左对齐  默认1  -->
	<!-- strFillContent 定长域补充内容      默认空格 -->
	<!-- strLength 长度 -->
	<!-- strSource 取值方式  $从平台中取 $$取默认值 -->
	<!-- strDestKey 平台取值字段名称-->
	<!-- strDefaultValue 默认值-->
	<!-- 与蔡总确定，20170713短信通修改交易不上送IC、密码、磁道数据 -->
  <request>
	<field description="报文类型标识符">
		<id>F0</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>4</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>0013</strDefaultValue>
	</field>
	<field description="客户号">
		<id>F1</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>19</strLength>
		<strSource>$</strSource>
		<strDestKey>strCustomerId</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="卡号">
		<id>F2</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>19</strLength>
		<strSource>$</strSource>
		<strDestKey>strPan</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="交易处理码">
		<id>F3</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>6</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransCode</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="系统跟踪号">
		<id>F11</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strTerminalTsn</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="受卡方所在地时间 hhmmss">
		<id>F12</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>6</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransTime</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="受卡方所在地日期 yyyyMMdd">
		<id>F13</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>8</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransDate</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="服务点输入方式码">
		<id>F22</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>3</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>022</strDefaultValue>
	</field>
	<field description="受卡机终端标识码 机具编号">
		<id>F41</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>8</strLength>
		<strSource>$</strSource>
		<strDestKey>strTerminalNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="证件编号">
	    <id>F61_1</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strIDCardNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="持卡人姓名">
	    <id>F61_6_NM</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strIDName</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="自定义域">
	    <id>F60</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>999</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>operationType,mobile,signMobile,mandatoryMobile,elementValue1,elementValue2,IMG_BATCHNO</strDefaultValue>
	</field>
	<field description="影像批次号">
	    <id>IMG_BATCHNO</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>20</strLength>
		<strSource>$</strSource>
		<strDestKey>strBatchId</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="操作类型">
	    <id>operationType</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>999</strLength>
		<strSource>$</strSource>
		<strDestKey>strAcctType</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="使用其他手机号">
	    <id>mobile</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>999</strLength>
		<strSource>$</strSource>
		<strDestKey>strPhone</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="手机号">
	    <id>signMobile</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>999</strLength>
		<strSource>$</strSource>
		<strDestKey>strSignMobile</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="强制通知手机号">
	    <id>mandatoryMobile</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>999</strLength>
		<strSource>$</strSource>
		<strDestKey>strMandatoryMobile</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="发生额下限">
	    <id>elementValue1</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>999</strLength>
		<strSource>$</strSource>
		<strDestKey>strOpenLimitAmount</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="是否显示余额">
	    <id>elementValue2</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>999</strLength>
		<strSource>$</strSource>
		<strDestKey>strIsShowBalance</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="客户流水号(业务批次号)">
	    <id>F71</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$</strSource>
		<strDestKey>strSingleBusinessNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="是否记录交易流水">
		<id>F999</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>4</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>true</strDefaultValue>
	</field>
  </request>
  <TransCheckField>
    <field description="关键域检验">F2,F3,F11</field>
  </TransCheckField>
</MappingRule>