<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >

  <class name="com.yihuacomputer.cols.entity.CardUnitStatus" table="CardUnitStatus" dynamic-insert="true">
    <id name="id" type="int" column="id">
      <generator class="identity"/>
    </id>
    <property name="strTerminalNum" column="strTerminalNum" type="string"/>
	<property name="cuNum" column="iCuNum" type="int"/>
	<property name="strCuType" column="strCuType" type="string"/>
	<property name="initialCount" column="iInitialCount" type="int"/>
	<property name="curCount" column="iCurCount" type="int"/>
	<property name="strCuStatus" column="strCuStatus" type="string"/>
	<property name="strCardType" column="strCardType" type="string"/>
	<property name="strCardTrackStart" column="strCardTrackStart" type="string"/>
	<property name="strCardTrackEnd" column="strCardTrackEnd" type="string"/>
	<property name="cardSuccCount" column="iCardSuccCount" type="int"/>
	<property name="cardCaptureCount" column="iCardCaptureCount" type="int"/>
	<property name="cardDestroyCount" column="iCardDestroyCount" type="int"/>
	<property name="cardUnknown" column="iCardUnknown" type="int"/>
	<property name="cardTakenCount" column="iCardTakenCount" type="int"/>
	</class>
</hibernate-mapping>
