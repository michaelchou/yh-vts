<?xml version="1.0" encoding="UTF-8"?>
<MappingRule description="公共事业费代扣签约查询">
    <!-- id 唯一标识 -->
    <!-- strAlignType 定长域对齐方式，1：右对齐，2：左对齐  默认1  -->
	<!-- strFillContent 定长域补充内容      默认空格 -->
	<!-- strLength 长度 -->
	<!-- strSource 取值方式  $从平台中取 $$取默认值 -->
	<!-- strDestKey 平台取值字段名称-->
	<!-- strDefaultValue 默认值-->
  <request>
	<field description="报文类型标识符">
		<id>F0</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>4</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>0018</strDefaultValue>
	</field>
	<field description="客户号">
		<id>F1</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>6</strLength>
		<strSource>$</strSource>
		<strDestKey>strCustomerId</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="交易处理码">
		<id>F3</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>6</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransCode</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="系统跟踪号">
		<id>F11</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strTerminalTsn</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="受卡方所在地时间 hhmmss">
		<id>F12</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>6</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransTime</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="受卡方所在地日期 yyyyMMdd">
		<id>F13</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>8</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransDate</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="服务点输入方式码">
		<id>F22</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>3</strLength>
		<strSource>$</strSource>
		<strDestKey>strICFlag</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="卡序列号">
		<id>F23</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>3</strLength>
		<strSource>$</strSource>
		<strDestKey>str5F34</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="主账户">
		<id>F34</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>37</strLength>
		<strSource>$</strSource>
		<strDestKey>DestPan</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="受卡机终端标识码 机具编号">
		<id>F41</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>8</strLength>
		<strSource>$</strSource>
		<strDestKey>strTerminalNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="55域">
	    <id>F55</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>999</strLength>
		<strSource>$</strSource>
		<strDestKey>strField55</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="自定义域">
	    <id>F60</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>10</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>dkdkzl,dkdlgs,dksfcd,dkquha,IMG_BATCHNO</strDefaultValue>
	</field>
	<field description="影像批次号">
	    <id>IMG_BATCHNO</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>20</strLength>
		<strSource>$</strSource>
		<strDestKey>strBatchId</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="代扣种类">
	    <id>dkdkzl</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>10</strLength>
		<strSource>$</strSource>
		<strDestKey>strUserType</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="代扣公司">
	    <id>dkdlgs</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$</strSource>
		<strDestKey>strCompanyNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="用户编号">
	    <id>dksfcd</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>10</strLength>
		<strSource>$</strSource>
		<strDestKey>strUserNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="区号">
	    <id>dkquha</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>10</strLength>
		<strSource>$</strSource>
		<strDestKey>strAreaCode</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="客户流水号(业务批次号)">
	    <id>F71</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$</strSource>
		<strDestKey>strSingleBusinessNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="是否记录交易流水">
		<id>F999</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>4</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>false</strDefaultValue>
	</field>
  </request>
  <TransCheckField>
    <field description="关键域检验">F3,F11</field>
  </TransCheckField>
</MappingRule>