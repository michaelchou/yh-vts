<?xml version="1.0" encoding="UTF-8"?>
<MappingRule description="存款利率">
    <!-- id 唯一标识 -->
    <!-- strAlignType 定长域对齐方式，1：右对齐，2：左对齐  默认1  -->
	<!-- strFillContent 定长域补充内容      默认空格 -->
	<!-- strLength 长度 -->
	<!-- strSource 取值方式  $从平台中取 $$取默认值 -->
	<!-- strDestKey 平台取值字段名称-->
	<!-- strDefaultValue 默认值-->
  <request>
	<field description="报文类型标识符">
		<id>F0</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>4</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>0003</strDefaultValue>
	</field>
	<field description="客户号">
		<id>F1</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>6</strLength>
		<strSource>$</strSource>
		<strDestKey>strCustomerId</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>	
	<field description="交易处理码">
		<id>F3</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>6</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransCode</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>	
	<field description="系统跟踪号">
		<id>F11</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strTerminalTsn</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="受卡方所在地时间 hhmmss">
		<id>F12</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>6</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransTime</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="受卡方所在地日期 yyyyMMdd">
		<id>F13</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>8</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransDate</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="服务点输入方式码">
		<id>F22</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>3</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>000</strDefaultValue>
	</field>	
	<field description="受卡机终端标识码 机具编号">
		<id>F41</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>8</strLength>
		<strSource>$</strSource>
		<strDestKey>strTerminalNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="币种">
	    <id>F49</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$</strSource>
		<strDestKey>strCurrency</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="是否计算上浮利率">
	    <id>F57</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$</strSource>
		<strDestKey>strRateUp</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="60域">
	    <id>F60</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>currCode,productType1,productType2,productType3,productType4,productType5,productType6,productType7,productType8,productType9,IMG_BATCHNO</strDefaultValue>
	</field>
	<field description="影像批次号">
	    <id>IMG_BATCHNO</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>20</strLength>
		<strSource>$</strSource>
		<strDestKey>strBatchId</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="币种">
	    <id>currCode</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>DR010</strDefaultValue>
	</field>
	<field description="产品类型1：三个月">
	    <id>productType1</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>DR01R002</strDefaultValue>
	</field>
	<field description="产品类型2：六个月">
	    <id>productType2</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>DR01R003</strDefaultValue>
	</field>
	<field description="产品类型3：一年">
	    <id>productType3</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>DR01R004</strDefaultValue>
	</field>
	<field description="产品类型4：两年">
	    <id>productType4</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>DR01R005</strDefaultValue>
	</field>
	<field description="产品类型5：三年">
	    <id>productType5</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>DR01R006</strDefaultValue>
	</field>
	<field description="产品类型6：五年">
	    <id>productType6</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>DR01R007</strDefaultValue>
	</field>
	<field description="产品类型7">
	    <id>productType7</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="产品类型8">
	    <id>productType8</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="产品类型9">
	    <id>productType9</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="业务批次号">
	    <id>F71</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransRandom</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="是否记录交易流水">
		<id>F999</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>4</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>false</strDefaultValue>
	</field>	
  </request>
  <TransCheckField>
    <field description="关键域检验">F3,F11</field>
  </TransCheckField>
</MappingRule>
