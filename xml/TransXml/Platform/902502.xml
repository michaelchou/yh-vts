		<id>F41</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>8</strLength>
		<strSource>$</strSource>
		<strDestKey>strTerminalNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="附加数据-对私">
		<id>F48</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>8</strLength>
		<strSource>$</strSource>
		<strDestKey>strPwdFlag</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="个人标识码数据">
		<id>F52</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>32</strLength>
		<strSource>$</strSource>
		<strDestKey>strPinBlock</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="安全控制信息 06-国际算法 04-国密算法">
	    <id>F53</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strEncrypType</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="55域">
	    <id>F55</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>999</strLength>
		<strSource>$</strSource>
		<strDestKey>strField55</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="57域">
	    <id>F57</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>999</strLength>
		<strSource>$</strSource>
		<strDestKey>strVoucherNo</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="自定义域">
	    <id>F60</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>999</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>mobileCall,birthdayDt,voucherType,IMG_BATCHNO</strDefaultValue>
	</field>
	<field description="移动电话">
	    <id>mobileCall</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>11</strLength>
		<strSource>$</strSource>
		<strDestKey>strMobileCall</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="出生日期">
	    <id>birthdayDt</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>11</strLength>
		<strSource>$</strSource>
		<strDestKey>strIDBorn</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="凭证类型存单折时必输;2001 个人存单2201 活期一本通2202 定期一本通 该值取自第三磁道">
	    <id>voucherType</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>11</strLength>
		<strSource>$</strSource>
		<strDestKey>strVoucherType</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="影像批次号">
	    <id>IMG_BATCHNO</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>20</strLength>
		<strSource>$</strSource>
		<strDestKey>strBatchId</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="证件编号">
	    <id>F61_1</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>19</strLength>
		<strSource>$</strSource>
		<strDestKey>strIDCardNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="持卡人姓名">
	    <id>F61_6_NM</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>19</strLength>
		<strSource>$</strSource>
		<strDestKey>strIDName</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="客户流水号(业务批次号)">
	    <id>F71</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$</strSource>
		<strDestKey>strSingleBusinessNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="是否记录交易流水">
		<id>F999</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>4</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>true</strDefaultValue>
	</field>
  </request>
  <TransCheckField>
  	<!-- 去掉2域关键域,存折、存单不存在2域 -->
    <field description="关键域检验">F3,F11</field>
  </TransCheckField>
</MappingRule>