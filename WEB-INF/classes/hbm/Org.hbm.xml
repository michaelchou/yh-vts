<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >

  <class name="com.yihuacomputer.cols.entity.Org" table="Org" dynamic-insert="true">
    <id name="id" type="int" column="id">
      <generator class="identity"/>
    </id>
	<property name="strParentOrgCode" column="strParentOrgCode" type="string"/>
    <property name="strOrgNum" column="strOrgNum" type="string"/>
	<property name="strOrgName" column="strOrgName" type="string"/>
	</class>
</hibernate-mapping>
