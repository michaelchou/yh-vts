<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >

  <class name="com.yihuacomputer.cols.entity.UKeySettleCycleLog" table="UKeySettleCycleLog" dynamic-insert="true">
    <id name="id" type="int" column="id">
      <generator class="identity"/>
    </id>
    <property name="strTerminalNum" column="strTerminalNum" type="string"/>
	<property name="termBatchNo" column="iTermBatchNo" type="int"/>
	<property name="dtStart" column="dtStart" type="timestamp"/>
	<property name="dtEnd" column="dtEnd" type="timestamp"/>
	<property name="status" column="iStatus" type="int"/>
	<property name="uKeySurplusCount" column="iUKeySurplusCount" type="int"/>
	<property name="uKeyRefillCount" column="iUKeyRefillCount" type="int"/>
	</class>
</hibernate-mapping>
