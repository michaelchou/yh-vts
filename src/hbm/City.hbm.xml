<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >

  <class name="com.yihuacomputer.cols.entity.City" table="City" dynamic-insert="true">
    <id name="id" type="int" column="id">
      <generator class="identity"/>
    </id>
    <property name="strProvinceCode" column="strProvinceCode" type="string" not-null="true" />
	<property name="strCityCode" column="strCityCode" type="string" not-null="true" />
	<property name="strCityName" column="strCityName" type="string" not-null="true" />
	</class>
</hibernate-mapping>
