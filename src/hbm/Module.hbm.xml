<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >

  <class name="com.yihuacomputer.cols.entity.Module" table="Module" dynamic-insert="true">
    <id name="id" type="int" column="id">
      <generator class="identity"/>
    </id>
    <property name="strModuleName" column="strModuleName" type="string" />
	<property name="strModuleClsid" column="strModuleClsid" type="string" />
	<property name="strModuleDesc" column="strModuleDesc" type="string" />
	</class>
</hibernate-mapping>
