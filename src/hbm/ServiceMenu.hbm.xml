<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.yihuacomputer.cols.entity.ServiceMenu" table="ServiceMenu" lazy="false">
		<id name="id" type="int" column="id">
			<generator class="identity" />
		</id>
		<property name="serviceMenuId" column="iServiceMenuId" type="int"/>
		<property name="strServiceMenuName" column="strServiceMenuName" type="string" not-null="true" />
		<property name="strServiceMenuNameEn" column="strServiceMenuNameEn" type="string" not-null="true" />
		<property name="btnPos" column="iBtnPos" type="int" />
		<property name="serviceMenuType" column="iServiceMenuType" type="int" />
		<property name="strServiceMenuAction" column="strServiceMenuAction" type="string" not-null="true" />
		<property name="strOrgNum" column="strOrgNum" type="string" not-null="true" />
		<property name="strCardType" column="strCardType" type="string" />
	</class>
</hibernate-mapping>
