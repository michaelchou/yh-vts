<?xml version="1.0" encoding="UTF-8"?>
<MappingRule description="对公存款确认">
    <!-- id 唯一标识 -->
    <!-- strAlignType 定长域对齐方式，1：右对齐，2：左对齐  默认1  -->
	<!-- strFillContent 定长域补充内容      默认空格 -->
	<!-- strLength 长度 -->
	<!-- strSource 取值方式  $从平台中取 $$取默认值 -->
	<!-- strDestKey 平台取值字段名称-->
	<!-- strDefaultValue 默认值-->
  <request>
	<field description="报文类型标识符">
		<id>F0</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>4</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>0021</strDefaultValue>
	</field>
	<field description="主账号(收款人账号)">
		<id>F34</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>37</strLength>
		<strSource>$</strSource>
		<strDestKey>strAccount</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>	
	<field description="交易处理码">
		<id>F3</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>6</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransCode</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="交易金额">
		<id>F4</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>12</strLength>
		<strSource>$</strSource>
		<strDestKey>Amount</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="系统跟踪号">
		<id>F11</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strTerminalTsn</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="受卡方所在地时间 hhmmss">
		<id>F12</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>6</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransTime</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="受卡方所在地日期 yyyyMMdd">
		<id>F13</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>8</strLength>
		<strSource>$</strSource>
		<strDestKey>strTransDate</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>	
	<field description="受卡机终端标识码 机具编号">
		<id>F41</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>8</strLength>
		<strSource>$</strSource>
		<strDestKey>strTerminalNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="证件编号">
	    <id>F61_1</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strIDCardNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="自定义域">
	    <id>F60</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>Y100,Y50,Y20,Y10,Y5,Y1,C50,C10,C5,C1,F90,payName,IMG_BATCHNO</strDefaultValue>
	</field>
	<field description="影像批次号">
	    <id>IMG_BATCHNO</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>20</strLength>
		<strSource>$</strSource>
		<strDestKey>strBatchId</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="100元张数">
	    <id>Y100</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strDepY100</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="50元张数">
	    <id>Y50</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strDepY50</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="20元张数">
	    <id>Y20</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strDepY20</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="10元张数">
	    <id>Y10</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strDepY10</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="5元张数">
	    <id>Y5</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strDepY5</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="1元张数">
	    <id>Y1</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strDepY1</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="5角张数">
	    <id>C50</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strDepC50</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="1角张数">
	    <id>C10</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strDepC10</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="5分张数">
	    <id>C5</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strDepC5</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="1分张数">
	    <id>C1</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strDepC1</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="原存款流水号">
	    <id>F90</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strOrgTsn</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="解款人（付款人）名称">
	    <id>payName</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>16</strLength>
		<strSource>$</strSource>
		<strDestKey>strPayerName</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="客户流水号(业务批次号)">
	    <id>F71</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>100</strLength>
		<strSource>$</strSource>
		<strDestKey>strSingleBusinessNum</strDestKey>
		<strDefaultValue></strDefaultValue>
	</field>
	<field description="是否记录交易流水">
		<id>F999</id>
	    <strAlignType>1</strAlignType>
	    <strFillContent></strFillContent>
		<strLength>4</strLength>
		<strSource>$$</strSource>
		<strDestKey></strDestKey>
		<strDefaultValue>true</strDefaultValue>
	</field>	
  </request>
  <TransCheckField>
    <field description="关键域检验">F3,F11</field>
  </TransCheckField>
</MappingRule>