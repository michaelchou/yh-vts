<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.yihuacomputer.cols.entity.CardTypeService" table="CardTypeService" lazy="false">
		<id name="id" type="int" column="id">
			<generator class="identity" />
		</id>
		<property name="strCardType" column="strCardType" type="string" />
		<property name="serviceMenuId" column="iServiceMenuId" type="int" />
	</class>
</hibernate-mapping>
